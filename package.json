{
  "name": "react-namespaces",
  "version": "1.3.0",
  "description": "A react namespace component. Make context-aware components easily!",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "npm run test && npm run build",
    "prepublishOnly": "npm test",
    "postversion": "git push && git push --tags",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biels/react-namespaces.git"
  },
  "keywords": [
    "react",
    "namespaces",
    "component"
  ],
  "author": "biels",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/biels/react-namespaces/issues"
  },
  "homepage": "https://github.com/biels/react-namespaces#readme",
  "devDependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.4.0",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.8.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "rootDir": "src",
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>setupTests.ts",
    "globals": {
      "ts-jest": {
        "skipBabel": true,
        "enableTsDiagnostics": false
      }
    }
  }
}
